    package="com.example.user.lolascupcakes">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login" />
        <activity android:name=".userRegister" />
        <activity android:name=".AdminHome" />
        <activity android:name=".memberHome" />
        <activity android:name=".addCupCakes" />
        <activity android:name=".placingOrder" />
        <activity android:name=".cakeList" />
        <activity android:name=".viewCakeList" />
        <activity android:name=".orderList"></activity>
    </application>

</manifest>